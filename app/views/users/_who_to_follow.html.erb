<h3>Who to Follow</h3><br>


<% User.all.order(created_at: :desc).each do |user| %>
  <div class="d-flex">
    <div>
      <%= link_to users_show_path(user.id) do %>
      <image class="profile-picture3"<%= image_tag display_photo %>
    </div>
    <div>
      <%= user.fullname %><br/>
      <small>@<%= user.username %></small><br/><br>
    <% end %>
    </div>
  </div>
<% end %>
